{
  "name": "muri",
  "version": "0.1.0",
  "description": "Fast unused file detection for JavaScript/TypeScript projects",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "muri": "cli.js"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "cli.js",
    "native.js",
    "native.d.ts",
    "*.node"
  ],
  "keywords": [
    "unused",
    "files",
    "dead-code",
    "javascript",
    "typescript",
    "lint",
    "analyzer"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/muri"
  },
  "engines": {
    "node": ">=20"
  },
  "optionalDependencies": {
    "muri-darwin-x64": "0.1.0",
    "muri-darwin-arm64": "0.1.0",
    "muri-linux-x64-gnu": "0.1.0",
    "muri-linux-arm64-gnu": "0.1.0",
    "muri-win32-x64-msvc": "0.1.0"
  }
}
